{
  "name": "@0x1luxa/utils",
  "version": "0.2.18",
  "description": "A collection of utility functions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./web3": {
      "import": "./dist/utils/web3/index.js",
      "types": "./dist/utils/web3/index.d.ts"
    },
    "./common": {
      "import": "./dist/utils/common/index.js",
      "types": "./dist/utils/common/index.d.ts"
    },
    "./date": {
      "import": "./dist/utils/date/index.js",
      "types": "./dist/utils/date/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "patch-push": "npm version patch && git push --tags",
    "build": "tsc"
  },
  "keywords": [
    "utils",
    "helpers",
    "utility"
  ],
  "author": "0x1luxa",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22",
    "typescript": "^5"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "dayjs": "^1.11.13",
    "tsup": "^8.4.0",
    "viem": "^2.23.12"
  }
}
